@page "/"

<Terminy OnCurrentTerminChanged="CurrentTerminChangedAsync"  />

<Prihlaseni @ref="prihlaseniComponent"  />
@*<Neprihlaseni />  *@


@functions
	{
		private Prihlaseni prihlaseniComponent;

		async Task CurrentTerminChangedAsync(int terminId)
		{
			await prihlaseniComponent.SetCurrentTermin(terminId);
		}
	}

